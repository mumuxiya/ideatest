<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lg.jabcmysqldemo.dao.PermissionsDao">

    <select id="selectByRoleId" resultType="Permissions">
        select p.* from t_permission p,t_role_permission rp where rp.role_id=#{id} AND p.id=rp.permission_id
    </select>
</mapper>
